jumpDelay = 30;
jumpDuration = 70;
counter = 0;
gridSize = 4;

var newColor = irandom(2);
newX = 0;
newY = 0;
gridCounterX = 0;
gridCounterY = 1;

//centerX = global.ResolutionX/2;
centerY = global.ResolutionY/2;
offSetY = centerY - (global.ResolutionY - global.ResolutionX)/2;
spacing = global.ResolutionX/gridSize + 1;

for(i = 0; i &lt; (gridSize * gridSize); i++){
    newColor = irandom(2);
    gridCounterX += 1;
    if(gridCounterX &gt; gridSize){
        gridCounterX = 1;
        gridCounterY += 1;
    }
    
    newX = spacing * gridCounterX - spacing/2;
    newY = offSetY + spacing * gridCounterY;
    
    //to store new fish
    var newFish = instance_create(newX,newY,obj_fish);
    var newSprite;

    newSprite = scr_setFishSprite(newColor);

    //set the new fish's properties
    with(newFish){
        myColor = newColor;
        sprite_index = newSprite;
    }
}



