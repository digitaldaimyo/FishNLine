if(counter &gt; jumpDelay + jumpDuration){
    with(obj_fish){
        JUMPING = false;
        visible = false;
        newColor = irandom(2);
        sprite_index = scr_setFishSprite(newColor);
        myColor = newColor;
        
        newSplash = instance_create(self.x,self.y,obj_splash);
    }
    counter = 0;
}else if(counter = jumpDelay){
    with(obj_fish){
        JUMPING = true;
        visible = true;
        
        newSplash = instance_create(self.x,self.y,obj_splash);
    }
}

global.sizematters = ds_list_size(global.nodeList);
global.slotone = ds_list_find_value(global.nodeList, 0);
